{
    "name": "coliving/commentable",
    "description": "Commentable Polymorphic Eloquent Models for for Laravel. This is fork from artisanry/commentable as it's abandoned",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Commentable",
        "Laravel Commentable"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Dzung Nguyen",
        "email": "dungnh@gmail.com",
        "homepage": "https://coliving.com"
    }],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0|^9.0",
        "illuminate/database": "^5.8|^6.0|^7.0|^8.0|^9.0",
        "kalnoy/nestedset": "^6.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.7",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "7.*|8.*|9.*"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Commentable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Commentable\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Commentable\\CommentableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
